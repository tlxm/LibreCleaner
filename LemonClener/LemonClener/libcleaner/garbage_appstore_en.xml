    <category id="1">
        <title><![CDATA[System Junks]]></title>
        <tips><![CDATA[Logs and caches generated by system]]></tips>
        <item id="1004" recommend="YES" fastmode="NO">
            <title><![CDATA[iOS Photo Caches]]></title>
            <tips><![CDATA[Cache files generated when you sync photos]]></tips>
            <action type="dir">
                <atom filters="(40+41)"/>
                <path type="abs" level="1" value="~/Pictures/iPod Photo Cache/"/>
            </action>
        </item>
        <item id="1005" recommend="YES" fastmode="NO">
            <title><![CDATA[iOS Application Update Caches]]></title>
            <tips><![CDATA[Cache files generated when you sync iOS and upgrade applications]]></tips>
            <action type="file">
                <path type="abs" filename=".+\.ipsw" level="-1" value="~/Library/iTunes/"/>
            </action>
        </item>
        <item id="1006" recommend="NO" fastmode="NO">
            <title><![CDATA[Trash]]></title>
            <tips><![CDATA[Please make sure all the files in your trash can be removed before you select it]]></tips>
            <action type="file" cleanemptyfolder="YES" cleanhiddenfile="YES">
                <path type="abs" level="1" value="~/.Trash/"/>
            </action>
        </item>
    </category>
    
    
    <category id="2">
        <title><![CDATA[Application Junks]]></title>
        <tips><![CDATA[Remove caches and logs generated by applications]]></tips>
        <item id="2101" fastmode="YES" showaction="YES" defaultstate="-1" bundleid="com.apple.dt.Xcode">
            <title><![CDATA[Xcode Junks]]></title>
            <tips><![CDATA[Select XCode caches and logs by default. Unfold to find more]]></tips>
            <action id="20011" type="appcache" sandbox="not">
                <title><![CDATA[Xcode app Caches]]></title>
                <path type="abs" level="1" value="~/Library/Caches/com.apple.dt.Xcode/"/>
                <path type="special" level="1" value="SystemTempDir" value1="com.apple.dt.Xcode"/>
            </action>
            <action id="20012" recommend="NO" type="derivedapp">
                <title><![CDATA[Xcode DerivedData]]></title>
                <path type="abs" filename=".+\.app" level="-1" value="~/Library/Developer/Xcode/DerivedData/"/>
            </action>
            <action id="20013" recommend="NO" type="file">
                <title><![CDATA[iOS Device Support]]></title>
                <path type="abs" level="1" value="~/Library/Developer/Xcode/iOS DeviceSupport/"/>
            </action>
            <action id="20014" recommend="NO" type="archive">
                <title><![CDATA[Archives]]></title>
                <path type="abs" filename=".+\.xcarchive" level="-1" value="~/Library/Developer/Xcode/Archives/"/>
            </action>
            <action id="20015" recommend="NO" type="file">
                <title><![CDATA[Module Caches]]></title>
                <path type="abs" level="0" value="~/Library/Developer/Xcode/DerivedData/ModuleCache.noindex/"/>
            </action>
            <action id="20016" recommend="NO" type="file">
                <title><![CDATA[iOS Device Logs]]></title>
                <path type="abs" level="0" value="~/Library/Developer/Xcode/iOS Device Logs/"/>
            </action>
            <action id="20017" recommend="NO" type="file">
                <title><![CDATA[DocumentationCache]]></title>
                <path type="abs" level="0" value="~/Library/Developer/Xcode/DocumentationCache/"/>
            </action>
        </item>
    </category>
    
    <category id="3">
        <title><![CDATA[Internet Junks]]></title>
        <tips><![CDATA[Clean the caches of Chrome, Opera, Safari Firefox and QQ browsers]]></tips>
        <item id="301" recommend="NO"  fastmode="NO">
            <title><![CDATA[Mail Attachments]]></title>
            <tips><![CDATA[Unwanted attachments can be removed]]></tips>
            <action type="file">
                <path type="abs" level="1" value="~/Library/Containers/com.apple.mail/Data/Library/Mail Downloads/"/>
            </action>
            <action type="mail">
                <path type="abs" level="1" value="~/Library/Mail/"/>
            </action>
        </item>
        <item id="306" recommend="YES"  showresult="YES" fastmode="NO" bundleid="com.apple.Safari">
            <title><![CDATA[Safari]]></title>
            <tips><![CDATA[Webpages or resources cached by Safari when loading pages]]></tips>
            <!--Internet Cache下的缓存文件-->
            <action id="3061" type="file" recommend="YES">
                <path type="abs" level="1" value="~/Library/Caches/com.apple.Safari/"/>
                <path type="abs" level="1" value="~/Library/Caches/Metadata/Safari/"/>
                <path type="abs" level="1" value="~/Library/Containers/com.apple.Safari.CacheDeleteExtension/"/>
                <path type="abs" level="1" value="~/Library/Caches/com.apple.Safari.SafeBrowsing/"/>
                <path type="abs" level="1" value="~/Library/Caches/com.apple.safaridavclient/"/>
            </action>
        </item>
        <item id="307" recommend="YES"  showresult="YES" fastmode="NO" bundleid="com.google.Chrome">
            <title><![CDATA[Chrome]]></title>
            <tips><![CDATA[Webpages or resources cached by Chrome when loading pages]]></tips>
            <!--Internet Cache-->
            <action id="3071" type="file" recommend="YES">
                <path type="abs" level="0" value="~/Library/Caches/Google/Chrome/Default/Cache/"/>
            </action>
        </item>
        <item id="308" recommend="YES"  showresult="NO" fastmode="NO" bundleid="com.tencent.QQBrowser">
            <title><![CDATA[QQBrowser]]></title>
            <tips><![CDATA[Webpages or resources cached by QQ Browser when loading pages]]></tips>
            <!--Internet Cache-->
            <action id="841" type="file" recommend="YES">
                <title><![CDATA[Browser Caches]]></title>
                <path type="abs" level="0" value="~/Library/Caches/QQBrowser2/Default/Cache/"/>
            </action>
        </item>
        <item id="309" recommend="YES"  showresult="NO" fastmode="NO" bundleid="com.operasoftware.Opera">
            <title><![CDATA[Opera]]></title>
            <tips><![CDATA[Webpages or resources cached by Opera when loading pages]]></tips>
            <!--Cache-->
            <action id="3091" type="file" recommend="YES" appversion="((1[0-2])|([0-9]))\..*" bundle="com.operasoftware.Opera" apppath="/Applications/Opera.app">
                <title><![CDATA[Browser Caches]]></title>
                <path type="abs" level="0" value="~/Library/Caches/Opera/"/>
            </action>
            <action id="3092" type="file" recommend="YES" appversion="((1[3-9])|([2-9][0-9]))\..*" bundle="com.operasoftware.Opera" apppath="/Applications/Opera.app">
                <title><![CDATA[Browser Caches]]></title>
                <path type="abs" level="0" value="~/Library/Application Support/com.operasoftware.Opera/GPUCache"/>
                <path type="abs" level="0" value="~/Library/Caches/com.operasoftware.Opera"/>
            </action>
            <!--Application Cache-->
            <action id="3093" type="file" recommend="YES" appversion="((1[0-2])|([0-9]))\..*" bundle="com.operasoftware.Opera" apppath="/Applications/Opera.app">
                <title><![CDATA[Applications Caches]]></title>
                <path type="abs" level="0" value="~/Library/Application Support/Opera/application_cache"/>
            </action>
        </item>
        <item id="310" recommend="YES"  showresult="NO" fastmode="NO" bundleid="org.mozilla.firefox">
            <title><![CDATA[FireFox]]></title>
            <tips><![CDATA[Webpages or resources cached by FireFox when loading pages]]></tips>
            <!--~/Library/Caches/Google/Chrome/Default/Cache/下的缓存文件-->
            <action id="3101" type="file" recommend="YES">
                <title><![CDATA[Browser Cache]]></title>
                <path type="abs" level="0" value="~/Library/Caches/Firefox/"/>
            </action>
        </item>
    </category>
    
</garbage>


